<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XF.MVVMBasic.View.MainPage">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
            <Button x:Name="btnCadastrar" Text="Cadastrar Aluno" Clicked="Cadastrar_Clicked" />
            <Button x:Name="btnVisualizar" Text="Visualizar Alunos" Clicked="Visualizar_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>